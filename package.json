{
  "name": "edulint-vscode",
  "displayName": "EduLint",
  "description": "EduLint VS Code Extension for students at the Faculty of Informatics, Masaryk Uni",
  "icon": "assets/icon.png",
  "publisher": "matousekm",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.82.0"
  },
  "license": "gpl-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/matousekm/edulint-vscode"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "edulint.lint",
        "title": "EduLint: Run Linting"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
